msgid "FTP couldn't use REST\n"
msgstr "FTP ne peut utiliser la commande d'attente (REST)\n"

#: ../curl_download/curl_download.xs:228
msgid "FTP couldn't get size\n"
msgstr "FTP : impossible de connaître la taille\n"

