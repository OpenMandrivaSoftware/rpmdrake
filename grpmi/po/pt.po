#: ../grpmi.pl_.c:65 ../grpmi.pl_.c:119 ../grpmi.pl_.c:145
msgid "Yes"
msgstr "Sim"

#: ../grpmi.pl_.c:66 ../grpmi.pl_.c:119 ../grpmi.pl_.c:120 ../grpmi.pl_.c:145
#: ../grpmi.pl_.c:146
msgid "No"
msgstr "Não"

#: ../grpmi.pl_.c:67
msgid "Ok"
msgstr "Ok"

#: ../grpmi.pl_.c:74
msgid "Error..."
msgstr "Erro..."

